{
  "name": "serene",
  "version": "0.0.4",
  "description": "(WIP) minimal & modular front-end for bittorrent + webtorrent daemons",
  "scripts": {
    "start": "webpack-dev-server -d",
    "test": "echo \"Error: no test specified\" && exit 1",
    "provider:add": "npm install -g --prefix ./plugins",
    "provider:install": "npm run provider:add",
    "provider:remove": "npm remove -g --prefix ./plugins",
    "build": "NODE_ENV=production webpack -p"
  },
  "repository": "sidd/serene",
  "keywords": [
    "serene",
    "torrent",
    "bittorrent",
    "webtorrent",
    "webui"
  ],
  "author": "Sidd Sridharan <sidd@sidd.com> (http://sidd.com)",
  "license": "MIT",
